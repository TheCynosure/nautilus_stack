<launch>
  <node name="nautilus" pkg="nautilus" type="nautilus" args="--config_file=$(find nautilus)/config/jackal_2020_config.lua" required="true" output="screen"/>
  <node name="laser_scan_matcher" pkg="laser_scan_matcher" type="match_laser_scans.py" args="$(find laser_scan_matcher)/models/ 16"/>
  <node name="local_uncertainty_estimator" pkg="local_uncertainty_estimator" type="estimate_local_uncertainty.py" args="$(find local_uncertainty_estimator)/models/ 44"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find nautilus)/slam_debug.rviz" />
</launch>
